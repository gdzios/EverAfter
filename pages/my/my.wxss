.header-container {
  display: flex;
  flex-direction: column;
  height: 300rpx;
  background-color: #ff9b6e;
  align-items: center;
  justify-content: center;
}

.header-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.header-nickname {
  color: #FFF;
  font-size: 30rpx;
}

.item-container {
  display: flex;
  flex-direction: row;
  background-color: #FFF;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 25rpx;
  align-items: center;
}

.item-icon {
  height: 50rpx;
  width: 50rpx;
}

.item-label {
  flex: 1;
  font-size: 30rpx;
  color: #000;
  margin-left: 25rpx;
}
.banquan{
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
  font-size: 20rpx;
  color: #9d9d9d;
}
.image{
   width: 100%;
   display: flex;
   justify-content: center;
   align-items: center;
   position: fixed;
   bottom: 1;
 
}